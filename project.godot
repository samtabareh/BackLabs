; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="BackLabs"
config/version="0.3.2"
run/main_scene="res://Scenes/Main Menu/Main.tscn"
config/use_custom_user_dir=true
config/features=PackedStringArray("4.2", "GL Compatibility")
boot_splash/show_image=false
config/icon="res://icon.svg"

[autoload]

Main="*res://Scripts/Main.gd"
TypesLoader="*res://Scripts/TypesLoader.gd"
LevelHandler="*res://Scripts/LevelHandler.gd"
SaveHandler="*res://Scripts/SaveHandler.gd"
InputHandler="*res://Scripts/InputHandler.gd"
TranslationSwitcher="*res://Scripts/TranslationSwitcher.gd"

[debug]

file_logging/enable_file_logging=true
gdscript/warnings/unused_parameter=0
gdscript/warnings/shadowed_variable=0
gdscript/warnings/shadowed_variable_base_class=0
gdscript/warnings/shadowed_global_identifier=0

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/size/mode=3
window/size/resizable=false
window/stretch/mode="viewport"
window/stretch/aspect="expand"

[editor]

movie_writer/movie_file="D:/Others/Godot Projects/Prototype/Run.avi"
movie_writer/fps=30

[file_customization]

folder_colors={
"res://Assets/": "yellow",
"res://Objects/": "green",
"res://Resources/": "purple",
"res://Scenes/": "blue",
"res://Scripts/": "teal",
"res://addons/": "red"
}

